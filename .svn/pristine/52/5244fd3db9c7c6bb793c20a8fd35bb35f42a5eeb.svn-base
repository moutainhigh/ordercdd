<div class="wrapper">
    <div class="row">
        <div class="col-xs-12 col-sm-10">
            <div class="panel panel-info">
                <div class="panel-heading">
                    销售产品
                </div>
                <table class="table table-striped m-b-none">
                    <tbody>
                    <tr>
                        <td class="v-middle text-right w-sm">产品名称：</td>
                        <td class="v-middle w-xl"  >{{ profile.product.label }}</td>
                        <td class="v-middle text-right w-sm">还款方式：</td>
                        <td class="v-middle w-xl">{{ allEntitiesInfo.product.repaymentMethodCode.values[profile.repaymentMethodCode].label }}</td>
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">给客户的年利率：</td>
                        <td class="v-middle w-xl">{{ profile.product.debtorInterest?(profile.product.debtorInterest*100 + '%'):''}}</td>
                        <td class="v-middle text-right w-sm">给门店的年利率：</td>
                        <td class="v-middle w-xl">{{ profile.product.storeInterest?(profile.product.storeInterest*100 +'%'):""}}</td>
                        <!--<td class="v-middle text-right w-sm">最小借款：</td>-->
                        <!--<td class="v-middle w-xl">{{ profile.product.storeAmountMin }}</td>-->
                        <!--<td class="v-middle text-right w-sm">最大借款：</td>-->
                        <!--<td class="v-middle" >{{ profile.product.storeAmountMax }}</td>-->
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">分期数：</td>
                        <td class="v-middle" colspan="3">{{ profile.product.periodNumber }}</td>
                    </tr>

                    </tbody>
                </table>
            </div>

            <div class="panel panel-success">
                <div class="panel-heading">
                    门店收费项目
                </div>
                <table class="table table-striped m-b-none">
                    <thead>
                    <tr>
                        <th></th>
                        <th>收费项目名称</th>
                        <th>分期</th>
                        <th>分期数</th>
                        <th>收费方式</th>
                        <th>数值</th>
                        <th>是否退回</th>
                        <th>说明</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="fee in feetypeStore" >
                        <td class="v-middle">
                        </td>
                        <td class="v-middle">
                            {{ fee.label }}
                        </td>
                        <td class="v-middle">
                            {{ fee.isPeriod == "true" ? "分期":"不分期" }}
                        </td>
                        <td class="v-middle">
                            {{ fee.periodNumber | nullToNumber}}
                        </td>
                        <td class="v-middle">
                            {{ fee.isRepayment == "false" ? "固定收费(元)"  : "按比例收费" }}
                        </td>
                        <td class="v-middle" >
                            {{ fee.feeVal | nullToNumber }}
                        </td>
                        <td class="v-middle">
                            {{ fee.isRefund == "true" ? "可退":"不可退" }}
                        </td>
                        <td class="v-middle w-xl">
                            {{ fee.comment }}
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>

            <div class="panel panel-success">
                <div class="panel-heading">
                    平台收费项目
                </div>
                <table class="table table-striped m-b-none">
                    <thead>
                    <tr>
                        <th></th>
                        <th>收费项目名称</th>
                        <th>分期</th>
                        <th>分期数</th>
                        <th>收费方式</th>
                        <th>数值</th>
                        <th>是否退回</th>
                        <th>说明</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="fee in feetypes">
                        <td class="v-middle">
                        </td>
                        <td class="v-middle">
                            {{ fee.label }}
                        </td>
                        <td class="v-middle">
                            {{ fee.isPeriod == "true" ? "分期":"不分期" }}
                        </td>
                        <td class="v-middle">
                            {{ fee.periodNumber | nullToNumber }}
                        </td>
                        <td class="v-middle">
                            {{ fee.isRepayment == "false" ? "固定收费(元)"  : "按比例收费" }}
                        </td>
                        <td class="v-middle" >
                            {{ fee.feeVal | nullToNumber }}
                        </td>
                        <td class="v-middle">
                            {{ fee.isRefund == "true" ? "可退":"不可退" }}
                        </td>
                        <td class="v-middle w-xl">
                            {{ fee.comment }}
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

