<div class="vbox bg-white">
    <div class="wrapper bg-white b-b clearfix">
        <ul class="nav nav-pills nav-sm">
            <li ui-sref-active="active">
                <a ui-sref="workflowEntity.home.profile.info">主页</a>
            </li>
            <li ui-sref-active="active">
                <a ui-sref="workflowEntity.home.profile.page({entity_key:'creditrepayment',view_type:'file'})">申请文件</a>
            </li>
            <li ui-sref-active="active">
                <a ui-sref="workflowEntity.home.profile.page({entity_key:'creditrepayment',view_type:'log'})">操作记录</a>
            </li>
        </ul>
    </div>
    <div class="wrapper b-b b-light clearfix" ng-if="canActs.length > 0">
        <button ng-class="act.btnClass"
                ng-click="creditrepaymentExcute[act.actCode](profile)"
                tooltip="{{act.help}}"
                tooltip-placement="bottom"
                ng-repeat="act in canActs">
            <i ng-class="act.iconClass"></i> {{act.label}}
        </button>
    </div>
    <div class="row-row">
        <div class="cell scrollable">
            <div class="cell-inner" ui-view=""></div>
        </div>
    </div>
</div>