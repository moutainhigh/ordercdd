<div class="wrapper">
    <div class="row">
        <div class="col-xs-12 col-sm-10">
            <div class="panel panel-info">
                <div class="panel-heading">
                    贷款信息
                </div>
                <table class="table table-striped m-b-none">
                    <tbody>
                    <tr>
                        <td class="v-middle text-right w-sm">借款总额：</td>
                        <td class="v-middle w-xl" >{{ profileDetail.applyAmount }}</td>
                        <td class="v-middle text-right w-sm">借款单号：</td>
                        <td class="v-middle w-xl">{{ profileDetail.loan.loanSn?profileDetail.loan.loanSn:'客户未借款' }}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<div class="wrapper">
    <div class="row">
        <div class="col-xs-12 col-sm-10">
            <div class="panel panel-info">
                <div class="panel-heading">
                    车辆信息
                </div>
                <table class="table table-striped m-b-none">
                    <tbody>
                    <tr>
                        <td class="v-middle text-right w-sm">车牌号：</td>
                        <td class="v-middle w-xl">{{ profileDetail.applyPlateNumber }}</td>
                        <td class="v-middle text-right w-sm">车辆型号：</td>
                        <td class="v-middle w-xl">{{ profileDetail.applyVehicleModel }}</td>
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">上牌区域：</td>
                        <td class="v-middle w-xl">{{profileDetail.vehicleProvince}} {{profileDetail.vehicleCity}}</td>
                        <td class="v-middle text-right w-sm">车辆年份：</td>
                        <td class="v-middle w-xl">{{profileDetail.vehicleDate }}</td>
                        <!--<td class="v-middle text-right w-sm">销售产品：</td>-->
                        <!--<td class="v-middle w-xl">{{ profile.product.label }}</td>-->
                        <!--<td class="v-middle text-right w-sm">年检到期日：</td>-->
                        <!--<td class="v-middle">{{ profile.applyAnnualSurveyMaturity?(profile.applyAnnualSurveyMaturity |-->
                        <!--datetime):"没有填时间" }}-->
                        <!--</td>-->
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">公里数：</td>
                        <td class="v-middle w-xl">{{ profileDetail.vehicleKm }} 万公里</td>
                        <td class="v-middle text-right w-sm"></td>
                        <td class="v-middle w-xl">{{  }}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<div class="wrapper">
    <div class="row">
        <div class="col-xs-12 col-sm-10">
            <div class="panel panel-info">
                <div class="panel-heading">
                    销售产品
                </div>
                <table class="table table-striped m-b-none">
                    <tbody>
                    <tr>
                        <td class="v-middle text-right w-sm">产品名称：</td>
                        <td class="v-middle w-xl"  >{{ profileDetail.product.label }}</td>
                        <td class="v-middle text-right w-sm">还款方式：</td>
                        <td class="v-middle w-xl">{{ allEntitiesInfo.product.repaymentMethodCode.values[profileDetail.repaymentMethodCode].label }}</td>
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">给客户的年利率：</td>
                        <td class="v-middle w-xl">{{ profileDetail.product.debtorInterest?(profileDetail.product.debtorInterest*100 + '%'):''}}</td>
                        <td class="v-middle text-right w-sm">给门店的年利率：</td>
                        <td class="v-middle w-xl">{{ profileDetail.product.storeInterest?(profileDetail.product.storeInterest*100 +'%'):""}}</td>
                        <!--<td class="v-middle text-right w-sm">最小借款：</td>-->
                        <!--<td class="v-middle w-xl">{{ profile.product.storeAmountMin }}</td>-->
                        <!--<td class="v-middle text-right w-sm">最大借款：</td>-->
                        <!--<td class="v-middle" >{{ profile.product.storeAmountMax }}</td>-->
                    </tr>
                    <tr>
                        <td class="v-middle text-right w-sm">分期数：</td>
                        <td class="v-middle" colspan="3">{{ profileDetail.product.periodNumber }}</td>
                    </tr>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

