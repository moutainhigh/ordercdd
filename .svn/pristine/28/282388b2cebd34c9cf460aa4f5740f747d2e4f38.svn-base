                initGetGroupMemberTable([]);
                var options = {
                    'GroupId': group_id,
                    'Offset': 0, //必须从0开始
                    'Limit': totalCount,
                    'MemberInfoFilter': [
                        'Account',
                        'Role',
                        'JoinTime',
                        'LastSendMsgTime',
                        'ShutUpUntil'
