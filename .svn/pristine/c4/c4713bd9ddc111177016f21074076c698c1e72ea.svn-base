<script>
    "use strict";
    app.controller("loanListController",
        [
            "$rootScope",
            "$scope",
            "$http",
            "entity",
            "$stateParams",
            function ($rootScope, $scope, $http, entity, $stateParams) {

                $scope.states = [];

                /*搜索值的对象*/
                $scope.searchForms = {};

                $scope.forms = {};

                /*初始值*/
                $scope.forms.state_id = $scope.state_id;
                $scope.forms.size     = "10";
                $scope.forms.sort     = "id,desc";


                $scope.exceptionReson = {};
                // 异常催收原因

                $scope.exceptionArr = [
                    {   exception:"gps异常"
                    },
                    {
                        exception:"车辆二抵"

                    },
                    {
                        exception:"其他"
                    }
                ];
//                $scope.exceptionReson.exception = $scope.exceptionArr[0];

                entity.getList("/rest/loanWorkflows/ " + $stateParams.workflow_id + "/states").then(function (resResult) {
                    $scope.states = resResult._embeddedItems;
                    if ($scope.states.length > 0) {
                        $scope.states     = $scope.states.sort(function (a, b) {
                            return a.sort - b.sort;
                        });

                        if (window.sessionStorage.getItem(window.myinfo.id + "loan_state_code")) {
                            angular.forEach($scope.states, function (val, key) {
                                if (val.id == window.sessionStorage.getItem(window.myinfo.id + "loan_state_code")) {
                                    $scope.stateVal       = $scope.states[key];
                                    $scope.forms.state_id = val.id;
                                    $scope.state_id       = val.id;
                                    $scope.getList(0);
                                }
                            })
                        } else {
                            $scope.forms.state_id = null;
                            $scope.getList(0);
                        }



//                        $scope.state_code = "NOTLEND";
//                        $scope.getList(0);
                    }
                });

                $scope.pageList = {
                    size: 0,
                    total_elements: 0,
                    current_page: 0,
                    num_page: 1,
                    total_page: 1
                };

                $scope.list = [];

                $scope.getList = function (page) {

                    $scope.loading = true;

                    $scope.list = [];

                    $scope.forms.page = page;


                    $http.get("/loan/search", {
                        params: $scope.forms
                    }).success(function (resResult) {
                        console.log(resResult,'loan')

                        $scope.loading = false;

                        $scope.list = entity.getListActs(resResult.entitys);

                        if ($scope.list.length > 0) {
                            $scope.pageList = {
                                size: resResult.size,
                                total_elements: resResult.totalElements,
                                current_page: resResult.number,
                                num_page: resResult.totalPages,
                                total_page: resResult.totalPages
                            };
                        } else {
                            $scope.pageList = {
                                size: 0,
                                total_elements: 0,
                                current_page: 0,
                                num_page: 1,
                                total_page: 1
                            };
                        }

                        $rootScope.setPaginator("#loan_list_paginator", $scope.pageList, $scope.getList);
                    });
                };

                //状态
                $scope.stateChange = function (val) {
                    $scope.forms = {};
                    $scope.forms.size     = "10";
                    $scope.forms.sort     = "id,desc";
                    if(val){
                        window.sessionStorage.setItem(window.myinfo.id + "loan_state_code", val.id);
                        $scope.state_id = val.id;

                        $scope.forms.state_id = val.id;
                        // $scope.getList(0);
                    }else {

                        window.sessionStorage.setItem(window.myinfo.id + "ordercdd_state_code", "");
                        $scope.state_id = null;
                        $scope.forms.state_id = null;
                        // $scope.getList(0);
                        // console.log($scope.forms);
                    }
                };

                //                搜索
                $scope.searchSubmit = function () {

                    if($scope.searchForms.startDate){
                        $scope.searchForms.timeRangeKey = "loanTime";
                    }

                    if($scope.searchForms.endDate){
                        $scope.searchForms.timeRangeKey = "loanTime";
                    }

                    /*合并对象*/
                    $scope.forms = Object.assign($scope.searchForms,$scope.forms);

                    $scope.getList(0)

                };

                //清除搜索
                $scope.clearSearch = function () {
                    $scope.forms = {};
                    $scope.searchForms = {};

                    if($scope.state_id){
                        $scope.forms.state_id = $scope.state_id;
                    }else {
                        $scope.forms.state_id = null;
                    }

                    $scope.forms.size     = "10";
                    $scope.forms.sort     = "id,desc";
                    $scope.getList(0);
                };

                //excel导出
                $scope.ExcelDownload=function () {
                    $http.get("/loan/export", {
                        params: $scope.forms,
                        responseType: 'arraybuffer'
                    }).success(function (resResult) {
                        var blob = new Blob([resResult], { type: "application/vnd.ms-excel" });
                        var objectUrl = URL.createObjectURL(blob);

                        var aForExcel = $("<a><span class='forExcel'>下载excel</span></a>")
                            .attr("href", objectUrl)
                            .attr("download",moment().format("YYYY-MM-DD")+".xls");
                        $("body").append(aForExcel);
                        $(".forExcel").click();
                        aForExcel.remove();
                    })
                };

                $scope.startDateToday = function () {
                    $scope.searchForms.startDate = new Date();
                };

                $scope.startDateClear = function () {
                    $scope.searchForms.startDate = null;
                };

                $scope.endDateToday = function () {
                    $scope.searchForms.endDate = new Date();
                };

                $scope.endDateClear = function () {
                    $scope.searchForms.endDate = null;
                };

                $scope.openStart = function ($event) {
                    $event.preventDefault();
                    $event.stopPropagation();

                    $scope.startOpened = true;
                    $scope.endOpened   = false;
                };

                $scope.openEnd = function ($event) {
                    $event.preventDefault();
                    $event.stopPropagation();

                    $scope.startOpened = false;
                    $scope.endOpened   = true;
                };

                $scope.dateOptions = {
                    formatYear: "yy",
                    startingDay: 1,
                    class: "datepicker",
                    showWeeks:false
                };


//                $scope.getList = function (page) {
//                    $scope.loading = true;
//
//                    $scope.list = [];
//
//                    //var link = "/rest/loans/search/listOwnDepartmentAndChildren?size=20&page=" + page;
//
//                    //if($scope.state_code){
//                    var link = "/rest/loans/search/listStateOwnDepartmentAndChildren?sort=id,desc&stateCode=" + $scope.state_code + "&size=10&page=" + page;
//                    //}
//
////                    entity.getList(link, ["ordercdd"]).then(function (resResult) {
//                    entity.getList(link,["files","createdBy"]).then(function (resResult) {
//                        $scope.loading = false;
//
//                        $scope.list = resResult._embeddedItems;
//
//                        console.log( $scope.list)
//                        if ($scope.list.length > 0) {
//                            $scope.pageList = {
//                                size: resResult.page.size,
//                                total_elements: resResult.page.totalElements,
//                                current_page: resResult.page.number,
//                                num_page: resResult.page.totalPages,
//                                total_page: resResult.page.totalPages
//                            };
//                        }else{
//                            $scope.pageList = {
//                                size: 0,
//                                total_elements: 0,
//                                current_page: 0,
//                                num_page: 1,
//                                total_page: 1
//                            };
//                        }
//
//                        $rootScope.setPaginator("#loan_list_paginator", $scope.pageList, $scope.getList);
//
//                    });
//                };

//                if ($scope.state_code) {
//                    $scope.getList(0);
//                }

//                $scope.stateSelect = function ($event, stateCode) {
//                    if ($event.target.nodeName === "INPUT") {
//                        $scope.state_code = stateCode;
//                        $scope.getList(0);
//                    }
//                };

            }
        ]
    );
</script>
