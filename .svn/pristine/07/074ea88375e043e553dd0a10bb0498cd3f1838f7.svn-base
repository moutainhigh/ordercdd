<div class="vbox">
    <div class="wrapper b-b">
        <div class="font-thin h4">风控查询</div>
    </div>
    <div class="row-row">
        <div class="cell scrollable">
            <div class="cell-inner">
                <div class="wrapper-md">
                    <div class="col-sm-8">
                        <p class="m-b">营业执行号/身份证号/手机号</p>
                        <input type="text" name="realname" placeholder="营业执行号/身份证号/手机号" class="form-control" ng-model="realname" required>
                        <Br />
                        <button type="submit" class="btn btn-primary">智能查询</button>
                        <button type="submit" class="btn btn-default"><i class="fa fa-file-pdf-o"></i> 生成PDF</button>
                        <button type="submit" class="btn btn-default">关联到标的？</button>
                    </div>
                    <div class="col-sm-4">
                        <p class="m-b">
                            选择自动分析的项目
                        </p>
                        <div>
                            <div class="checkbox">
                                <label class="i-checks">
                                    <input type="checkbox" value="">
                                    <i></i>
                                    个人资料综合分析
                                </label>
                            </div>
                            <div class="checkbox">
                                <label class="i-checks">
                                    <input type="checkbox" checked value="">
                                    <i></i>
                                    手机号综合分析
                                </label>
                            </div>
                            <div class="checkbox">
                                <label class="i-checks">
                                    <input type="checkbox" checked value="">
                                    <i></i>
                                    身份证号综合分析
                                </label>
                            </div>
                            <div class="checkbox">
                                <label class="i-checks">
                                    <input type="checkbox" checked value="">
                                    <i></i>
                                    银行与消费综合分析
                                </label>
                            </div>
                            <div class="checkbox">
                                <label class="i-checks">
                                    <input type="checkbox" checked value="">
                                    <i></i>
                                    信用类综合分析
                                </label>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


