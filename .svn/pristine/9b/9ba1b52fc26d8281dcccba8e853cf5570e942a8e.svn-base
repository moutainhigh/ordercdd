<script>
    "use strict";
    app.controller("capitalplanController",
        [
            "$rootScope",
            "$scope",
            "$http",
            "entity",
            "$stateParams",
            function ($rootScope, $scope, $http, entity, $stateParams) {

                $scope.states = [];

                /*搜索值的对象*/
                $scope.searchForms = {};

                $scope.forms = {};

                /*初始值*/
                $scope.forms.state_id = $scope.state_id;
                $scope.forms.size     = "10";
                $scope.forms.sort     = "id,desc";

                entity.getList("/rest/capitalplanWorkflows/ " + $stateParams.workflow_id + "/states").then(function (resResult) {
                    $scope.states = resResult._embeddedItems;
                    if ($scope.states.length > 0) {
                        $scope.states     = $scope.states.sort(function (a, b) {
                            return a.sort - b.sort;
                        });

                        $scope.stateVal = $scope.states[1];

                        $scope.forms = {};
                        $scope.forms.size     = "10";
                        $scope.forms.sort     = "id,desc";

                        $scope.state_id = $scope.stateVal.id;

                        $scope.forms.state_id = $scope.stateVal.id;
                        $scope.getList(0);
//                        if (window.sessionStorage.getItem(window.myinfo.id + "capitalplan_state_code")) {
//                            angular.forEach($scope.states, function (val, key) {
//                                if (val.id == window.sessionStorage.getItem(window.myinfo.id + "capitalplan_state_code")) {
//                                    $scope.stateVal       = $scope.states[key];
//                                    $scope.forms.state_id = val.id;
//                                    $scope.state_id       = val.id;
//                                    $scope.getList(0);
//                                }
//                            })
//                        } else {
//                            $scope.forms.state_id = null;
//                            $scope.getList(0);
//                        }
                    }
                });

                $scope.pageList = {
                    size: 0,
                    total_elements: 0,
                    current_page: 0,
                    num_page: 1,
                    total_page: 1
                };

                $scope.list = [];

                $scope.getList = function (page) {

                    $scope.loading = true;

                    $scope.list = [];

                    $scope.forms.page = page;


                    $http.get("/capitalplan/search", {
                        params: $scope.forms
                    }).success(function (resResult) {

                        console.log(resResult,'resResult')
                        $scope.loading = false;

                        $scope.list = entity.getListActs(resResult.entitys);

                        if ($scope.list.length > 0) {
                            $scope.pageList = {
                                size: resResult.size,
                                total_elements: resResult.totalElements,
                                current_page: resResult.number,
                                num_page: resResult.totalPages,
                                total_page: resResult.totalPages
                            };
                        } else {
                            $scope.pageList = {
                                size: 0,
                                total_elements: 0,
                                current_page: 0,
                                num_page: 1,
                                total_page: 1
                            };
                        }

                        $rootScope.setPaginator("#capitalplan_list_paginator", $scope.pageList, $scope.getList);
                    });
                };

                //状态
                $scope.stateChange = function (val) {
                    $scope.forms = {};
                    $scope.forms.size     = "10";
                    $scope.forms.sort     = "id,desc";
                    if(val){
                        window.sessionStorage.setItem(window.myinfo.id + "capitalplan_state_code", val.id);

                        $scope.state_id = val.id;
                        $scope.forms.state_id = val.id;
//                        $scope.getList(0);
//                        $scope.searchSubmit();
                    }else {
                        window.sessionStorage.setItem(window.myinfo.id + "capitalplan_state_code", "");

                        $scope.state_id = null;
                        $scope.forms.state_id = null;
//                        $scope.getList(0);
//                        $scope.searchSubmit();
                    }
                };

                //                搜索
                $scope.searchSubmit = function () {

                    //是否逾期
                    if($scope.isOverdueVal === true ){
                        $scope.searchForms.overdue = $scope.isOverdueVal;
                    }else if($scope.isOverdueVal === false){
                        $scope.searchForms.overdue = $scope.isOverdueVal;
                    }
                    else{
                        $scope.searchForms.overdue = undefined;
                    }

                    /*合并对象*/
                    $scope.forms = Object.assign($scope.searchForms,$scope.forms);

                    $scope.getList(0)

                };

                $scope.setDateStr = function(AddDayCount) {
                    var dd = new Date();
                    var h = dd.getHours();
                    var m = dd.getMinutes();
                    var s = dd.getSeconds();
                    dd.setDate(dd.getDate()+AddDayCount);//获取AddDayCount天后的日期
                    var yy = dd.getFullYear();
                    var mm = (dd.getMonth()+1)<10?"0"+(dd.getMonth()+1):(dd.getMonth()+1);
                    var dd = dd.getDate()<10?"0"+dd.getDate():dd.getDate();
                    return yy+"-"+mm+"-"+dd+'T'+h+':'+m+':'+s+'.000Z';
                };

                //操作：最近三天还款计划
                $scope.nearlyrepayplan = function(){

                    var date = new Date();
                    var yy = date.getFullYear();
                    var mm = (date.getMonth()+1)<10?"0"+(date.getMonth()+1):(date.getMonth()+1);
                    var dd = date.getDate()<10?"0"+date.getDate():date.getDate();
                    var h = date.getHours();
                    var m = date.getMinutes();
                    var s = date.getSeconds();
                    var startDate = yy+'-'+mm+'-'+dd+'T'+h+':'+m+':'+s+'.000Z';

                    $scope.initForm = {
                        page : '0',
                        size : '10',
                        sort : 'id,desc',
                        startDate : startDate,
                        endDate : $scope.setDateStr(3),
                        timeRangeKey : 'debtorRepaymentDate'
                    }

                    $http.get("/creditrepayplan/search", {
                        params: $scope.initForm
                    }).success(function (resResult) {

                        $scope.loading = false;

                        $scope.list = entity.getListActs(resResult.entitys);

                        if ($scope.list.length > 0) {
                            $scope.pageList = {
                                size: resResult.size,
                                total_elements: resResult.totalElements,
                                current_page: resResult.number,
                                num_page: resResult.totalPages,
                                total_page: resResult.totalPages
                            };
                        } else {
                            $scope.pageList = {
                                size: 0,
                                total_elements: 0,
                                current_page: 0,
                                num_page: 1,
                                total_page: 1
                            };
                        }

                        $rootScope.setPaginator("#creditrepayplan_list_paginator", $scope.pageList, $scope.getList);
                    });
                };

                //清除搜索
                $scope.clearSearch = function () {
                    $scope.forms = {};
                    $scope.searchForms = {};

                    if($scope.state_id){
                        $scope.forms.state_id = $scope.state_id;
                    }else {
                        $scope.forms.state_id = null;
                    }

                    $scope.forms.size     = "10";
                    $scope.forms.sort     = "id,desc";
                    $scope.getList(0);
                };

                $scope.startDateToday = function () {
                    $scope.searchForms.startDate = new Date();
                };

                $scope.startDateClear = function () {
                    $scope.searchForms.startDate = null;
                };

                $scope.endDateToday = function () {
                    $scope.searchForms.endDate = new Date();
                };

                $scope.endDateClear = function () {
                    $scope.searchForms.endDate = null;
                };

                $scope.openStart = function ($event) {
                    $event.preventDefault();
                    $event.stopPropagation();

                    $scope.startOpened = true;
                    $scope.endOpened   = false;
                };

                $scope.openEnd = function ($event) {
                    $event.preventDefault();
                    $event.stopPropagation();

                    $scope.startOpened = false;
                    $scope.endOpened   = true;
                };

                $scope.dateOptions = {
                    formatYear: "yy",
                    startingDay: 1,
                    class: "datepicker",
                    showWeeks:false
                };


            }
        ]
    );
</script>
